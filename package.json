{
  "name": "jason-lang",
  "displayName": "JASON Lang",
  "description": "Language Support for the JASON file format",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.89.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "jason",
      "aliases": ["JASON", "jason"],
      "extensions": [".jason"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "jason",
      "scopeName": "source.jason",
      "path": "./syntaxes/jason.tmLanguage.json"
    }]
  }
}
